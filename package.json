{
  "name": "ziggy-react-app",
  "version": "1.0.0",
  "description": "simple boilerplate for react applications",
  "keywords": [
    "react",
    "boilerplate",
    "webpack",
    "babel",
    "sass",
    "hmr"
  ],
  "author": "Dan Nguyen",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziggysauce/ziggy-react-app.git"
  },
  "scripts": {
    "prestart": "npm run build:prod",
    "start": "node server/server.js",
    "test": "jest --i",
    "test:verbose": "jest -i --verbose",
    "test:coverage": "jest -i --coverage",
    "build:dev": "webpack-dev-server --hot --watch --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.19",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.12.0",
    "html-webpack-plugin": "^3.2.0",
    "npm": "^6.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  }
}
